for (XEntity entity : entities) {
    for (Map.Entry<String, List<String>> filterEntry : filterMap.entrySet()) {
        String attrName = filterEntry.getKey();
        List<String> allowedValues = filterEntry.getValue();

        String attrValue = null;
        switch (attrName) {
            case "a": attrValue = entity.getA(); break;
            case "b": attrValue = entity.getB(); break;
            case "c": attrValue = entity.getC(); break;
            case "d": attrValue = entity.getD(); break;
            case "e": attrValue = entity.getE(); break;
            default: continue; // ignore unknown
        }

        if (attrValue != null && allowedValues.contains(attrValue)) {
            result.put(attrValue, entity);
        }
    }
}

@Table(name = "vw_group_cost_aggregates")
@Immutable   // Hibernate annotation (important for views)
public class GroupCostAggregate {

    @Id
    @Column(name = "group_name")
    private String groupName;

    @Column(name = "wtd_cost")
    private BigDecimal wtdCost;

    @Column(name = "mtd_cost")
    private BigDecimal mtdCost;

    @Column(name = "qtd_cost")
    private BigDecimal qtdCost;

    @Column(name = "ytd_cost")
    private BigDecimal ytdCost;

String expiryStr = new SimpleDateFormat("yyyyMMdd").format(expiry.getTime());

        addOverride(overrides, "EXPIRATION_DATE", expiryStr);
        addOverride(overrides, "DELTA", "30");
        addOverride(overrides, "QUOTE_TYPE", "DELTA");
        addOverride(overrides, "QUOTE_SIDE", "PUT");
        addOverride(overrides, "IMPLIED_VOLATILITY_TYPE", "BLACK");
        addOverride(overrides, "UNDERLYING_PRICE_TYPE", "MID");

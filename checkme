@Scheduled(cron = "0 0 9 * * *")
    public void sendDailyTentativeNotifications() {
        LocalDate today = LocalDate.now();
        List<Rlh> todayList = rlhRepository.findByTentativeDate(today);
        emailService.sendTentativeDateNotification(todayList);
    }


Map<String, List<Xyz>> result = list.stream()
            .collect(Collectors.groupingBy(
                x -> firstNonNull(x.getA(), x.getB(), x.getC())
            ));
